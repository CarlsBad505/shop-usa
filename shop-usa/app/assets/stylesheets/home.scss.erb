$primary-color: #FED100;
$header-font: 'RalewayB';
$sub-header-font: 'RalewayR';

#inquiry-alrt-error-commercial {
  width: 50%;
  margin: auto;
  margin-bottom: 1em;
}

#inquiry-alrt-success-commercial {
  text-align: center;
}

.index-home {
  background: linear-gradient(to right, #fff, #E8E8E8);
}

.index-hiws {
  padding-top: 50px;
  background: linear-gradient(to right, #fff, #E8E8E8);
}

.index-enterprise {
  padding-top: 50px;
  background: linear-gradient(to right, #fff, #E8E8E8);
}

.products-gallery {
